<%= form_for([@shopping_cart, @cart_item = @shopping_cart.cart_items.build], :remote => true) do |f| %>
    <%= f.select(:sku) do %>
      <% array = [] %>
      <% drop_array = (1..10).to_a %>
      <% drop_array.each do |num| %>
      <% new_array = [num, cart_item.sku] %>
      <% array << new_array %>
      <% end %>
      <%= options_for_select(drop_array) %>
    <% end %>
    <%= f.submit 'UPDATE', :class => 'update_quantity_button' %>
<% end %>